\ProvidesPackage{beamercolorthemereMedian}
\definecolor{Purple}{RGB}{145,65,158}	% box1, block tile, list items
\definecolor{Blue}{RGB}{99,65,158} %box2
\definecolor{UltraLightPurple}{RGB}{242,222,247} %block text background
\definecolor{Alert}{RGB}{255,30,80}
\definecolor{Struct}{RGB}{32,160,255}

\setbeamercolor*{title page header}{fg=Purple, bg=UltraLightPurple}
\setbeamercolor*{title}{fg=Purple, bg=UltraLightPurple}
\setbeamercolor*{item}{fg=Purple}
\setbeamercolor*{frametitle}{fg=Purple!80!black}
\setbeamercolor*{block title}{fg=white, bg=Purple}
\setbeamercolor*{block title example}{fg=white, bg=Struct!70!black}
\setbeamercolor*{block title alerted}{fg=white, bg=Alert}
\setbeamercolor*{block body}{bg=UltraLightPurple}
\setbeamercolor*{block body example}{bg=Struct!20!white}
\setbeamercolor*{block body alerted}{bg=Alert!20!white}


\setbeamercolor*{normal text}{fg=Purple!20!black!80!gray}
\setbeamercolor*{section in toc}{fg=Purple!40!black}
\setbeamercolor*{alerted text}{fg=Alert}
\setbeamercolor*{structure}{fg=Struct}
\setbeamercolor*{page number in head/foot}{fg=Purple}


\setbeamercolor{palette primary}{bg=Purple, fg=white}
\setbeamercolor{palette quaternary}{bg=Blue, fg=white}
